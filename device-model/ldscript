TARGET(elf64-tradbigmips)
ENTRY(_start)

SECTIONS
{
	. = 0xffffffffb0000000;
	.start . : {
		*start.o(.text)
	}

	.text : {
		*(.exception)
		*(.text)
	}

	.rodata : {
		*(.rodata)
	}

	.data : {
		*(.data)
	}

	.sdata : {
		_gp = .;
		*(.sdata)
	}

	.bss : {
		*(.bss COMMON)
	}

	//. = ALIGN(4);
	//. = . + 0x1000; /* 4kB of stack memory */
	//stack_top = 0x80020000;
}
